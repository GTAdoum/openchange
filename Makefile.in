swigperl-all:
	@echo "Creating Perl bindings ..."
	@cd swig/perl && $(MAKE) all

swigperl-install:
	@echo "Install Perl bindings ..."
	@cd swig/perl && $(MAKE) install

swigperl-uninstall:
	@echo "Uninstall Perl bindings ..."
